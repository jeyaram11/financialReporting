					"transformations": [
						{
							"name": "derivedColumn1"
						},
						{
							"name": "input1"
						},
						{
							"name": "output1"
						}
					],
					"scriptLines": [
						"input(output(",
						"          PostalCode as string",
						"     ),",
						"     order: 0,",
						"     allowSchemaDrift: true) ~> input1",
						"input1 derive(PostalCode = lpad(PostalCode, 6, '0')) ~> derivedColumn1",
						"derivedColumn1 output() ~> output1"
					]
				}
			},
